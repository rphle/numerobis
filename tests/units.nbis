unit kt = 0.2g
echo(8kt -> kg)

# ---
from si import @(m, cm)
unit L: Volume = (0.1m)^3
unit mL: Volume = 0.001 L

echo(1mL == 1 cm^3)

# ---
unit mW = 0.001 W
unit dBm = 10^(_ mW / 10mW)

echo(2 * 60dBm)
echo(23W)
echo(60dBm |+| 60dBm)

# ---
dimension Temperature
unit K: Temperature
unit C: Temperature = _ K + 273.15;
unit F = (5/9) * (_ K + 459.67);

echo(5C)
echo(0C -> K)
echo(0C -> F)
echo(0C + 32F) # 0C + 0C
echo(0C |+| 32F)

