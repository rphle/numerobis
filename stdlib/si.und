# Common derived units with prefixes
unit kN: Force = 1e3 * newton
unit MN: Force = 1e6 * newton
unit kPa: Pressure = 1e3 * pascal
unit MPa: Pressure = 1e6 * pascal
unit GPa: Pressure = 1e9 * pascal
unit kJ: Energy = 1e3 * joule
unit MJ: Energy = 1e6 * joule
unit GJ: Energy = 1e9 * joule
unit kW: Power = 1e3 * watt
unit MW: Power = 1e6 * watt
unit GW: Power = 1e9 * watt
unit kV: ElectricPotential = 1e3 * volt
unit MV: ElectricPotential = 1e6 * volt
unit mV: ElectricPotential = 1e-3 * volt
unit µV: ElectricPotential = 1e-6 * volt
unit kΩ: ElectricResistance = 1e3 * ohm
unit MΩ: ElectricResistance = 1e6 * ohm
unit GΩ: ElectricResistance = 1e9 * ohm
unit mΩ: ElectricResistance = 1e-3 * ohm
unit µΩ: ElectricResistance = 1e-6 * ohm
unit µF: ElectricCapacitance = 1e-6 * farad
unit nF: ElectricCapacitance = 1e-9 * farad
unit pF: ElectricCapacitance = 1e-12 * farad
unit mH: Inductance = 1e-3 * henry
unit µH: Inductance = 1e-6 * henry
unit nH: Inductance = 1e-9 * henry

# Mass units (gram and derivatives)
unit gram: Mass = 1e-3 * kilogram
unit g: Mass = gram
unit µg: Mass = 1e-6 * gram
unit ng: Mass = 1e-9 * gram
unit pg: Mass = 1e-12 * gram
unit tonne: Mass = 1e3 * kilogram
unit t: Mass = tonne
unit Mt: Mass = 1e6 * kilogram
unit Gt: Mass = 1e9 * kilogram

# Volume units
unit litre: Volume = 1e-3 * metre^3
unit liter = litre  # American spelling
unit L: Volume = litre
unit mL: Volume = 1e-3 * litre
unit µL: Volume = 1e-6 * litre
unit nL: Volume = 1e-9 * litre
unit pL: Volume = 1e-12 * litre
unit kL: Volume = 1e3 * litre
unit ML: Volume = 1e6 * litre

unit cubic_metre: Volume = metre^3

# Time units
unit minute: Time = 60 * second
unit min: Time = minute
unit hour: Time = 60 * minute
unit h: Time = hour
unit hr: Time = hour
unit day: Time = 24 * hour
unit d: Time = day
unit week: Time = 7 * day
unit wk: Time = week
unit year: Time = 365.25 * day
unit yr: Time = year
unit century: Time = 100 * year
unit millennium: Time = 1000 * year

# Temperature units
unit celsius: ThermodynamicTemperature = kelvin  # Note: offset not handled
unit °C = celsius
unit fahrenheit = kelvin  # Note: offset and scale not handled
unit °F = fahrenheit

# Angle units
unit degree: Angle = 3.14159265359/180 * radian
unit °: Angle = degree
unit arcminute: Angle = degree/60
unit arcsecond: Angle = arcminute/60
unit gradian: Angle = 3.14159265359/200 * radian
unit gon: Angle = gradian
unit turn: Angle = 2 * 3.14159265359 * radian
unit revolution: Angle = turn
unit rev: Angle = revolution

# Frequency units
unit rpm: Frequency = 1/60 * hertz  # revolutions per minute
unit rps: Frequency = hertz  # revolutions per second
