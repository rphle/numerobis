
# SI (International System of Units) Standard Library
# Base dimensions

dimension Length
dimension Mass
dimension Time
dimension ElectricCurrent
dimension ThermodynamicTemperature
dimension AmountOfSubstance
dimension LuminousIntensity

# Derived dimensions
dimension Area = Length^2
dimension Volume = Length^3
dimension Velocity = Length / Time
dimension Acceleration = Length / Time^2
dimension Force = Mass * Acceleration
dimension Pressure = Force / Area
dimension Energy = Force * Length
dimension Power = Energy / Time
dimension ElectricCharge = ElectricCurrent * Time
dimension ElectricPotential = Power / ElectricCurrent
dimension ElectricResistance = ElectricPotential / ElectricCurrent
dimension ElectricConductance = 1 / ElectricResistance
dimension ElectricCapacitance = ElectricCharge / ElectricPotential
dimension MagneticFlux = ElectricPotential * Time
dimension MagneticFluxDensity = MagneticFlux / Area
dimension Inductance = MagneticFlux / ElectricCurrent
dimension Frequency = 1 / Time
dimension Angle = 1  # dimensionless
dimension SolidAngle = 1  # dimensionless
dimension LuminousFlux = LuminousIntensity * SolidAngle
dimension Illuminance = LuminousFlux / Area
dimension Density = Mass / Volume
dimension Momentum = Mass * Velocity
dimension AngularVelocity = Angle / Time
dimension AngularAcceleration = AngularVelocity / Time
dimension Torque = Force * Length
dimension Entropy = Energy / ThermodynamicTemperature
dimension SpecificHeatCapacity = Energy / (Mass * ThermodynamicTemperature)
dimension ThermalConductivity = Power / (Length * ThermodynamicTemperature)
dimension ElectricFieldStrength = Force / ElectricCharge
dimension MagneticFieldStrength = ElectricCurrent / Length

# SI Base Units
unit metre: Length
unit meter: Length = metre  # American spelling
unit m: Length = metre

unit kilogram: Mass
unit kg: Mass = kilogram

unit second: Time
unit s: Time = second

unit ampere: ElectricCurrent
unit amp: ElectricCurrent = ampere
unit A: ElectricCurrent = ampere

unit kelvin: ThermodynamicTemperature
unit K: ThermodynamicTemperature = kelvin

unit mole: AmountOfSubstance
unit mol: AmountOfSubstance = mole

unit candela: LuminousIntensity
unit cd: LuminousIntensity = candela

# SI Derived Units with Special Names
unit hertz: Frequency = 1 / second
unit Hz: Frequency = hertz

unit radian: Angle = 1
unit rad: Angle = radian

unit steradian: SolidAngle = 1
unit sr: SolidAngle = steradian

unit newton: Force = kilogram * metre / second^2
unit N: Force = newton

unit pascal: Pressure = newton / metre^2
unit Pa: Pressure = pascal

unit joule: Energy = newton * metre
unit J: Energy = joule

unit watt: Power = joule / second
unit W: Power = watt

unit coulomb: ElectricCharge = ampere * second
unit C: ElectricCharge = coulomb

unit volt: ElectricPotential = watt / ampere
unit V: ElectricPotential = volt

unit farad: ElectricCapacitance = coulomb / volt
unit F: ElectricCapacitance = farad

unit ohm: ElectricResistance = volt / ampere
unit Ω: ElectricResistance = ohm

unit siemens: ElectricConductance = 1 / ohm
unit S: ElectricConductance = siemens

unit weber: MagneticFlux = volt * second
unit Wb: MagneticFlux = weber

unit tesla: MagneticFluxDensity = weber / metre^2
unit T: MagneticFluxDensity = tesla

unit henry: Inductance = weber / ampere
unit H: Inductance = henry

unit lumen: LuminousFlux = candela * steradian
unit lm: LuminousFlux = lumen

unit lux: Illuminance = lumen / metre^2
unit lx: Illuminance = lux

# Metric Prefixes (powers of 10)
